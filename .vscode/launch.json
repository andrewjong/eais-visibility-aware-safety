    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: RaceCar Warm HJ",
            "type": "debugpy",
            "request": "launch",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "program": "${workspaceFolder}/run/racecar_warm_hj.py",
            "args": ["--world", "14x14Sparse"],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: RaceCar Warm HJ Dual",
            "type": "debugpy",
            "request": "launch",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "program": "${workspaceFolder}/run/racecar_warm_hj.py",
            "args": ["--planner", "dualguard_mppi", "--world", "14x14Sparse"],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal"
        },
    ]
}